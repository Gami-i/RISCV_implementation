(function() {
  var $, ErrorDisplay, Promise, ref;

  ref = [], $ = ref[0], Promise = ref[1];


  /*
  A module that displays a {Logging} error.
  
  @extend ModuleConfigObject
   */

  ErrorDisplay = (function() {
    function ErrorDisplay() {}

    ErrorDisplay.prototype.init = function(path) {
      this.path = path;
      return Promise.resolve(this);
    };

    ErrorDisplay.prototype.show = function(err) {
      if (err.retryCB != null) {
        if (confirm(err.usrMsg + " Retry?")) {
          return err.retryCB();
        }
      } else {
        return alert(err.usrMsg);
      }
    };

    return ErrorDisplay;

  })();

  define("ErrorDisplay", function(require, exports, module) {
    $ = require("jquery");
    Promise = require("bluebird");
    $.extend(ErrorDisplay.prototype, module.config());
    return ErrorDisplay;
  });

}).call(this);
