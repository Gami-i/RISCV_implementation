(function() {
  var $, BladeComponent, PanelLabel, ref;

  ref = [], BladeComponent = ref[0], $ = ref[1];


  /*
  Describes a floating menu to appear at the top right of a panel.
  
  @extend BladeComponent
  @extend ModuleConfigObject
   */

  PanelLabel = (function() {
    function PanelLabel() {}

    PanelLabel.prototype.el = null;

    PanelLabel.prototype.init = function(pane, options) {
      this.path = pane.childPath("label");
      this.hidden = false;
      options["path"] = this.path;
      return this.compileComponent("PanelLabel", options).then((function(_this) {
        return function(html) {
          _this.el = $(html);
          pane.paneEl.append(_this.el);
          return _this;
        };
      })(this))["catch"](function(err) {
        return console.log(err);
      });
    };


    /*
    Hides this label.
     */

    PanelLabel.prototype.hide = function() {
      if (this.hidden) {
        return;
      }
      return this.el.fadeTo(150, 0.2, "swing", (function(_this) {
        return function() {
          return _this.hidden = true;
        };
      })(this));
    };


    /*
    Shows this label.
     */

    PanelLabel.prototype.show = function() {
      if (!this.hidden) {
        return;
      }
      this.hidden = false;
      return this.el.fadeTo(150, 1.0, "swing", (function(_this) {
        return function() {
          return _this.hidden = false;
        };
      })(this));
    };

    return PanelLabel;

  })();

  define("PanelLabel", function(require, exports, module) {
    BladeComponent = require("BladeComponent");
    $ = require("jquery");
    $.extend(PanelLabel.prototype, BladeComponent, module.config());
    return PanelLabel;
  });

}).call(this);
